{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Dark mode for Trello",
  "version": "0.0.1",
  "description": "Enables dark mode for trello",
  "author": "JoelGamer",
  "web_accessible_resources": [
      "main.css"
  ],
  "content_scripts": [
      {
          "matches": [
              "https://trello.com/*"
          ],
          "js": [
              "index.js"
          ]
      }
  ]
}
